package Resources;

public class BMWMain {

    public static void main(String args[])
    {
        var bmw = new BMW(0,0,1);


        System.out.println("Umdrehungen pro Minute:" +
                bmw.GetRPM() + "\nGeschwindigkeit:" + 
                bmw.GetSpeed() + "\nGang:" + bmw.GetGear());
    }
}
